<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foode">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Navigation"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Cart" />
        <activity android:name=".Tea" />
        <activity android:name=".Patties" />
        <activity android:name=".Drinks" />
        <activity android:name=".Sandwich" />
        <activity android:name=".Shakes" />
        <activity android:name=".Samosa" />
        <activity android:name=".Menu" />
        <activity android:name=".GetStarted" />
        <activity android:name=".register" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>